{
  "entrypoint": "index.html",
  "shell": "src/app.main.js",
  "sources": [
    "src/**/*"
  ],
  "extraDependencies": [
    "node_modules/@webcomponents/webcomponentsjs/**"
  ],
  "builds": [{
    "name": "esm-bundled",
    "browserCapabilities": [
      "es2015",
      "modules"
    ],
    "js": {
      "minify": false
    },
    "css": {
      "minify": true
    },
    "html": {
      "minify": false
    },
    "bundle": false,
    "addServiceWorker": true
  }],
  "moduleResolution": "node",
  "npm": true
}